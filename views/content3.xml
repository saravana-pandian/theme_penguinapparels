<odoo>
<template id="s_content_products" name="content products">
      <section class="s_features pt32 pb32 o_colored_level">
        <div class="container">
        <div class="row">
            <div class="col-lg-9 pb16 o_colored_level">
              <h3 class="primary-heading">Products That We Offers</h3>
              <p class="primary-paragraph">It is With High Skill and almost attention to detail, we deliver a wide varity of Products in every segment we cater to. We are trusted by some of the biggest Barands in the apparel world</p>
            </div>
            <div class="col-lg-3 pt8 o_colored_level">
              <p class="centered">
                <a href="/contactus" class="primary-button">Full Gallery</a>
              </p>
            </div>
          </div>
          <div class="row">
            <div class="col-lg-4 pt32 pb32 text-center o_colored_level">
            <picture>
              <source media="(max-width: 991px) and (min-width: 768px)" srcset="/theme_penguin_apparels/static/src/images/Womens-Wear-Tablet-View.png">
                <source media="(min-width: 991px)" srcset="/theme_penguin_apparels/static/src/images/Womens-Wear-Web-View.png">
                  <source media="(max-width: 768px)" srcset="/theme_penguin_apparels/static/src/images/Womens-Wear-Mobile-View.png">
                    <img class="content-product-image" src="/theme_penguin_apparels/static/src/images/Womens-Wear-Web-View.png" alt="Womens Wear Product"/>
                    </source>
                </source>
              </source>
            </picture>
                <h3 class="primary-heading">Womens Wear</h3>
            </div>
            <div class="col-lg-4 pt32 pb32 text-center o_colored_level">
            <picture>
              <source media="(max-width: 991px) and (min-width: 768px)" srcset="/theme_penguin_apparels/static/src/images/Kids-wear-Tablet-View.png">
                <source media="(min-width: 991px)" srcset="/theme_penguin_apparels/static/src/images/Kids-Wear-Web-View.png">
                  <source media="(max-width: 768px)" srcset="/theme_penguin_apparels/static/src/images/Kids-Wear-Mobile-View.png">
                    <img class="content-product-image" src="/theme_penguin_apparels/static/src/images/Kids-Wear-Web-View.png" alt="Kids Wear Product"/>
              </source>
                </source>
              </source>
            </picture>
              <h3 class="primary-heading">Kids Wear</h3>
            </div>
            <div class="col-lg-4 pt32 pb32 text-center o_colored_level">
            <picture>
              <source media="(max-width: 991px) and (min-width: 768px)" srcset="/theme_Penguin/static/src/images/Mens-Wear-Tablet-View.png">
                <source media="(min-width: 991px)" srcset="/theme_penguin_apparels/static/src/images/Mens-Wear-Web-View.png">
                  <source media="(max-width: 768px)" srcset="/theme_penguin_apparels/static/src/images/Mens-Wear-Mobile-View.png">
                   <img class="content-product-image" src="/theme_penguin_apparelsn/static/src/images/Mens-Wear-Web-View.png" alt="Mens Wear Product"/>
              </source>
                </source>
              </source>
            </picture>
               <h3 class="primary-heading">Mens Wear</h3>
            </div>
          </div>
        </div>
      </section>
</template>
<template id="place_into_three" inherit_id="website.snippets" name="Place into three">
    <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
        <t t-snippet="theme_penguin_apparels.s_content_products" t-thumbnail="/theme_penguin_apparels/static/src/images/logos/PenguinApparels-Logo.png"/>
    </xpath>
</template>
</odoo>
